@{
    ViewBag.Title = "Test21";
    Layout = "~/Views/Home/Show.cshtml";
}
<html>
<head>
    <meta charset="UTF-8" />
    <title>Highcharts </title>
    <script src="http://apps.bdimg.com/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="~/Scripts/HighCharts/highcharts.js"></script>
    <script src="~/Scripts/HighCharts/exporting.js"></script>
    @*<script src="~/scripts/highcharts/grid-light.js"></script>*@
    <script src="http://code.highcharts.com/highcharts-3d.js"></script>
</head>
<body>
    <div id="container" style="width: 550px; height: 400px; margin: 0 auto"></div>
    <script language="JavaScript">
        $(document).ready(function () {
            var chart = {
                type: 'column',
                margin: 75,
                options3d: {
                    enabled: true,
                    alpha: 8,
                    beta: 14,
                    depth: 72
                }
            };
            var title = {
                text: '分时段蓄变量分析图'
            };
            var subtitle = {
                text: ''
            };
            var xAxis = {
                categories: ['2005-2006',
                        ' 2006-2007  ',
                        ' 2007-2008  ',
                        ' 2008-2009 ',
                        ' 2009-2010',
                        ' 2010-2011  '],// Highcharts.getOptions().lang.shortMonths
            };
            var yAxis = {
                title: {
                    text: '蓄变量（亿m<sup>3</sup>）'
                }
            };
            var series = [{
                name: '蓄变量',
                data: [-7.07  ,
                        -5.12,
                        2.05,
                        0.87,
                        0.87,
                        0.46

            ]
            }];

            var json = {};
            json.chart = chart;
            json.title = title;
            json.subtitle = subtitle;
            json.xAxis = xAxis;
            json.yAxis = yAxis;
            json.series = series;
            $('#container').highcharts(json);
        });
    </script>
</body>
</html>