@{
    ViewBag.Title = "Test14";
    Layout = "~/Views/Home/Show.cshtml";
}

<html>
<head>
    <meta charset="UTF-8" />
    <title>Highcharts</title>
  

    <script src="http://apps.bdimg.com/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="~/Scripts/HighCharts/highcharts.js"></script>
    <script src="~/Scripts/HighCharts/exporting.js"></script>
    <script src="~/scripts/highcharts/grid-light.js"></script>
</head>
<body>
    <div id="container" style="width: 550px; height: 400px; margin: 0 auto"></div>
    <script language="JavaScript">
        $(document).ready(function () {

            $('#container').highcharts({
                chart: { type: 'column' },
                title: { text: '地下水开采量时间序列图' },
                 colors : [
               '#FF0000',//红
              //'#FFFF00',//黄
             // '#F79646',// 橙黄
                         ],
                xAxis: {
                    categories: ['2005-1',
                        ' 2005-2  ',
                        ' 2005-3  ',
                        ' 2005-4  ',
                        ' 2005-5  ',
                        ' 2005-6  ',
                        ' 2005-7  ',
                        ' 2005-8  ',
                        ' 2005-9  ',
                        ' 2005-10 ',
                        ' 2005-11 ',
                        ' 2005-12 '],
                    title: {
                        enabled: true,
                        text: '日期',
                        x: 238,
                        y: -50
                    },
                    crosshair: true,

                },
                yAxis: {
                    title: {
                        text: '地下水开采量<span>（万m<sup>3</sup>）</span>'
                    },
                    plotLines: [{    //标识线
                        value: 0,
                        width: 1,
                        color: '#808080'
                    }]
                },
                tooltip: { valueSuffix: '' },
                series: [
                        {
          name: '地下水开采量',
          data: [793.66   ,
                753.36,
                1403.29,
                2711.88,
                2301.87,
                2151.36,
                1758.33,
                1691.37,
                1686.24,
                1509.38,
                2054.77,
                1495.10

                 ],
                    }
                ],

            });
 
});
    </script>
</body>
</html>